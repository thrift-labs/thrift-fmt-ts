{
  "name": "thrift-fmt",
  "type": "module",
  "version": "0.0.1",
  "description": "A thrift formatter, implement of python thrift-fmt",
  "main": "/src/index.ts",
  "files": [
    "lib/src/*",
    "src/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "test": "mocha -r ts-node/register 'test/*.ts'",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thrift-labs/thrift-fmt-ts.git"
  },
  "keywords": [
    "Thrift",
    "Formatter",
    "Thrift-fmt",
    "Pretty"
  ],
  "author": "alingse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thrift-labs/thrift-fmt-ts/issues"
  },
  "homepage": "https://github.com/thrift-labs/thrift-fmt-ts#readme",
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/chai": "^4.3.1",
    "@types/jest": "^28.1.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/antlr4": "^4.7.2",
    "thirft-parser-ts": "^1.0.0",
    "thrift-parser-antlr4": "^0.0.3"
  }
}
